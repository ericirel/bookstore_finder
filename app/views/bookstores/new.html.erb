<h1> New Bookstore </h1>
<%= form_for @bookstore do |f| %>
  <%= f.label :store %><br>
  <%= f.text_field :store %><br>
  <%= f.label :address %><br>
  <%= f.text_field :address %><br>
  <%= f.label :description %><br>
  <%= f.text_area :description %><br>
  <%= f.label :phone_number %><br>
  <%= f.text_field :phone_number %><br>
  <%= f.label :url %><br>
  <%= f.text_field :url %><br>
  <%= f.submit %>
  <% end %>
